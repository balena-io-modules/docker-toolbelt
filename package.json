{
  "name": "docker-toolbelt",
  "version": "6.0.14",
  "description": "Some tools for docker",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib tests",
    "lint-fix": "balena-lint --fix lib tests",
    "test": "npm run build && npm run lint && npm run test:fast",
    "test:fast": "mocha -r ts-node/register --reporter spec tests/**/*.spec.ts",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balena-io-modules/docker-toolbelt.git"
  },
  "author": "Balena Ltd.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/balena-io-modules/docker-toolbelt/issues"
  },
  "homepage": "https://github.com/balena-io-modules/docker-toolbelt#readme",
  "dependencies": {
    "balena-semver": "^3.0.10",
    "JSONStream": "^1.3.5",
    "randomstring": "^1.3.1",
    "tar-stream": "^3.1.7"
  },
  "devDependencies": {
    "@balena/lint": "^9.3.8",
    "@types/chai": "^4.3.20",
    "@types/dockerode": "^3.3.44",
    "@types/mocha": "^10.0.10",
    "@types/randomstring": "^1.3.0",
    "@types/tar-stream": "^3.1.4",
    "chai": "^4.5.0",
    "mocha": "^11.7.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.3"
  },
  "versionist": {
    "publishedAt": "2025-10-07T11:51:49.437Z"
  },
  "peerDependencies": {
    "dockerode": "^4.0.2"
  }
}
