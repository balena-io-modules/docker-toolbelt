    "@types/sinon": "^10.0.6",
    "chai": "^4.3.4",
    "mocha": "^8.4.0",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4"
  },
